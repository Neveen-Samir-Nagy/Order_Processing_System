<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ControllerSignIn">
   <children>
      <AnchorPane layoutX="10.0" layoutY="10.0">
         <children>
            <AnchorPane layoutX="-10.0" layoutY="-10.0" prefHeight="737.0" prefWidth="784.0" style="-fx-background-image: url(&quot;welcome2.JPG&quot;); -fx-background-repeat: stretch; -fx-background-size: cover; -fx-background-position: left top,center;">
               <children>
                  <Button fx:id="log_in" layoutX="291.0" layoutY="556.0" mnemonicParsing="false" onAction="#Sign_in" prefHeight="59.0" prefWidth="221.0" style="-fx-background-image: url(&quot;welcome.JPG&quot;);" text="LOG IN" textFill="DARKSLATEGRAY">
                     <font>
                        <Font name="Britannic Bold" size="28.0" />
                     </font>
                  </Button>
                  <TextField fx:id="user_name" alignment="CENTER" layoutX="194.0" layoutY="203.0" prefHeight="69.0" prefWidth="427.0" promptText="UserName OR EmailAddress">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </TextField>
                  <TextField fx:id="pass" alignment="CENTER" layoutX="194.0" layoutY="324.0" prefHeight="69.0" prefWidth="427.0" promptText="Password">
                     <font>
                        <Font size="29.0" />
                     </font>
                  </TextField>
                  <TextField fx:id="Type" alignment="CENTER" layoutX="194.0" layoutY="441.0" prefHeight="59.0" prefWidth="427.0" promptText="Type">
                     <font>
                        <Font size="29.0" />
                     </font>
                  </TextField>
                  <Button fx:id="register" layoutX="629.0" layoutY="25.0" mnemonicParsing="false" onAction="#sign_UP" prefHeight="59.0" prefWidth="145.0" style="-fx-background-image: url(&quot;welcome.JPG&quot;);" text="Register" textFill="DIMGRAY">
                     <font>
                        <Font name="Britannic Bold" size="25.0" />
                     </font>
                  </Button>
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
